<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.aplicativo2entregar">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="Validador de Nota"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Aplicativo2Entregar">
        <activity
            android:name=".MainActivity"
            android:parentActivityName="com.example.aplicativo2entregar.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Resultado"></activity>
    </application>

</manifest>